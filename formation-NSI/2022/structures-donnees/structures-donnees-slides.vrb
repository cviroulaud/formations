def prefixe(arbre: list, i: int = 0, p: list = []):
    if i < len(arbre):
        p.append(arbre[i])
        prefixe(arbre, 2*i+1, p)
        prefixe(arbre, 2*i+2, p)
    return p


arbre = [0, 1, 2, 3, 4, 5, 6]
parcours = prefixe(arbre)
parcours = prefixe(arbre)
